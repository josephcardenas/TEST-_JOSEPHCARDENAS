
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>

<body>

    <table class="table">
        <tr>
            <th>
                NOMBRE
            </th>
            <th>
                APELLIDO
            </th>
            <th>
                RAZON
            </th>
            <th>
                APLICALE
            </th>

        </tr>


        @foreach (var persona in Model)
        {
            <tr>
                <td>
                    @persona.Nombre
                </td>

                <td>
                    @persona.Apellido
                </td>

                <td>
                    @persona.Razon
                </td>

                <td>
                    @persona.Aplicale
                </td>






            </tr>



        }


            <button>
                
                GUARDAR<b></b>
           
            </button>









</body>
</html>
